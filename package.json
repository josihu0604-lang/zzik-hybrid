{
  "name": "zzik-hybrid",
  "version": "0.1.0",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "next dev --turbo",
    "dev:mobile": "next dev --turbo --hostname 0.0.0.0",
    "build": "next build --webpack",
    "pages:build": "npx @cloudflare/next-on-pages",
    "analyze": "ANALYZE=true next build",
    "build:analyze": "ANALYZE=true next build --webpack",
    "build:production": "NODE_ENV=production next build",
    "start": "next start",
    "lint": "eslint src --max-warnings 50",
    "lint:fix": "eslint src --fix --max-warnings 50",
    "format": "prettier --write \"src/**/*.{ts,tsx,js,jsx,json,css}\"",
    "format:check": "prettier --check \"src/**/*.{ts,tsx,js,jsx,json,css}\"",
    "type-check": "tsc --noEmit",
    "validate": "pnpm type-check && pnpm lint && pnpm test:run",
    "audit": "pnpm audit --audit-level=moderate",
    "audit:fix": "pnpm audit --fix",
    "test": "vitest",
    "test:run": "vitest run",
    "test:coverage": "vitest run --coverage",
    "test:e2e": "playwright test",
    "test:e2e:ui": "playwright test --ui",
    "test:e2e:headed": "playwright test --headed",
    "mcp:start": "./scripts/mcp/start-browser-tools.sh",
    "mcp:stop": "./scripts/mcp/stop-browser-tools.sh",
    "mcp:install-guide": "echo '브라우저에서 열기: file:///home/ubuntu/zzik-hybrid/scripts/mcp/install-extension.html' && xdg-open ./scripts/mcp/install-extension.html 2>/dev/null || open ./scripts/mcp/install-extension.html 2>/dev/null || echo '위 경로를 브라우저에 붙여넣으세요'",
    "uxui:server": "npx @agentdeskai/browser-tools-server@1.2.0",
    "uxui:info": "echo '\\n=== UXUI Audit Workflow ===\\n1. pnpm dev (개발 서버)\\n2. pnpm uxui:server (BrowserTools 서버 - 별도 터미널)\\n3. Chrome DevTools > BrowserToolsMCP 패널 열기\\n4. Claude에서 접근성/성능/SEO 검사 요청\\n'",
    "mobile:ip": "node scripts/get-local-ip.js",
    "mobile:setup": "node scripts/mobile-dev-setup.js",
    "prepare": "husky",
    "cap:init": "cap init",
    "cap:sync": "cap sync",
    "cap:copy": "cap copy",
    "cap:open:ios": "cap open ios",
    "cap:open:android": "cap open android",
    "cap:run:ios": "cap run ios",
    "cap:run:android": "cap run android",
    "cap:build": "next build && cap sync",
    "cap:icons": "npx @capacitor/assets generate --iconBackgroundColor '#0a0a0a' --splashBackgroundColor '#0a0a0a'",
    "mobile:sync": "cap sync",
    "mobile:ios": "cap sync && cap open ios",
    "mobile:android": "cap sync && cap open android",
    "mobile:run:ios": "CAPACITOR_DEV=true cap run ios --livereload --external",
    "mobile:run:android": "CAPACITOR_DEV=true cap run android --livereload --external",
    "mobile:build:ios": "cap sync ios && cd ios/App && xcodebuild -workspace App.xcworkspace -scheme App -configuration Release -archivePath build/ZZIK.xcarchive archive",
    "mobile:build:android": "cap sync android && cd android && ./gradlew assembleRelease",
    "tokenize:dry-run": "tsx scripts/tokenize-rgba.ts --dry-run",
    "tokenize": "tsx scripts/tokenize-rgba.ts",
    "research:market": "tsx scripts/business-research/market-research-agent.ts",
    "research:pricing": "tsx scripts/business-research/pricing-strategy-analyzer.ts",
    "research:all": "pnpm research:market && pnpm research:pricing"
  },
  "dependencies": {
    "@capacitor/camera": "^7.0.2",
    "@capacitor/geolocation": "^7.1.6",
    "@capacitor/preferences": "^7.0.2",
    "@capacitor/share": "^7.0.2",
    "@google/generative-ai": "^0.24.0",
    "@headlessui/react": "^2.2.9",
    "@privy-io/react-auth": "^1.58.0",
    "@privy-io/server-auth": "^1.5.0",
    "@sentry/nextjs": "^10.29.0",
    "@stripe/react-stripe-js": "^5.4.1",
    "@stripe/stripe-js": "^8.5.3",
    "@supabase/ssr": "^0.8.0",
    "@supabase/supabase-js": "^2.86.2",
    "@tanstack/react-query": "^5.90.12",
    "@tanstack/react-virtual": "^3.13.12",
    "@types/canvas-confetti": "^1.9.0",
    "axios": "^1.13.2",
    "canvas-confetti": "^1.9.4",
    "cheerio": "^1.1.2",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "csv-writer": "^1.6.0",
    "date-fns": "^4.1.0",
    "dotenv": "^17.2.3",
    "exceljs": "^4.4.0",
    "framer-motion": "^12.0.0",
    "lucide-react": "^0.556.0",
    "mapbox-gl": "^3.17.0",
    "next": "^16.0.7",
    "next-intl": "^4.5.8",
    "react": "^19.0.0",
    "react-dom": "^19.0.0",
    "react-icons": "^5.5.0",
    "react-map-gl": "^8.1.0",
    "server-only": "^0.0.1",
    "stripe": "^20.0.0",
    "tailwind-merge": "^3.0.0",
    "viem": "^2.7.0",
    "web-push": "^3.6.7",
    "web-vitals": "^5.1.0",
    "zod": "^3.23.8",
    "zustand": "^5.0.9"
  },
  "devDependencies": {
    "@capacitor/android": "^7.4.4",
    "@capacitor/app": "^7.1.0",
    "@capacitor/cli": "^7.4.4",
    "@capacitor/core": "^7.4.4",
    "@capacitor/haptics": "^7.0.2",
    "@capacitor/ios": "^7.4.4",
    "@capacitor/keyboard": "^7.0.3",
    "@capacitor/push-notifications": "^7.0.3",
    "@capacitor/splash-screen": "^7.0.3",
    "@capacitor/status-bar": "^7.0.3",
    "@cloudflare/next-on-pages": "^1.13.16",
    "@eslint/eslintrc": "^3.3.3",
    "@next/bundle-analyzer": "^16.0.7",
    "@playwright/test": "^1.57.0",
    "@tailwindcss/postcss": "^4.1.17",
    "@testing-library/dom": "^10.4.1",
    "@testing-library/jest-dom": "^6.9.1",
    "@testing-library/react": "^16.3.0",
    "@types/node": "^24.10.1",
    "@types/react": "^19.0.0",
    "@types/react-dom": "^19.0.0",
    "@types/web-push": "^3.6.3",
    "@vitejs/plugin-react": "^5.1.1",
    "@vitest/coverage-v8": "^4.0.15",
    "critters": "^0.0.25",
    "eslint": "^9.0.0",
    "eslint-config-next": "^16.0.7",
    "eslint-config-prettier": "^10.1.8",
    "husky": "^9.1.7",
    "import-in-the-middle": "^2.0.0",
    "jsdom": "^27.2.0",
    "lint-staged": "^16.2.7",
    "postcss": "^8.4.49",
    "prettier": "^3.7.4",
    "require-in-the-middle": "^8.0.1",
    "tailwindcss": "^4.1.17",
    "tsx": "^4.19.2",
    "typescript": "^5.7.0",
    "vercel": "^49.1.2",
    "vitest": "^4.0.15"
  },
  "lint-staged": {
    "*.{ts,tsx}": [
      "eslint --fix --max-warnings=50",
      "prettier --write"
    ],
    "*.{js,jsx,json,css,md}": [
      "prettier --write"
    ]
  }
}
